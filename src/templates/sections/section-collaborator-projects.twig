{#
title: Basic section
#}

{% set projects = site.data['collaborator-projects.json'] %}

{% embed "@templates/layouts/section.twig" %}

{% block header %}
<h3 class="section-title">Collaborator Projects</h3>
{% endblock %}

{% block content %}
<div class="news-content">
  {% for project in projects %}
    <div class="news-content-block">
      <div class="content-block-inner">
        {% include '@templates/partials/link-block.html' with {project} %}
      </div>
    </div>
  {% endfor %}
</div>
{% endblock %}

{% endembed %}